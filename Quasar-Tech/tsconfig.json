{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es6",
    "esModuleInterop": true,    
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "src/app/loadingcss.js", "src/app/TopBar.tsx", "src/app/SideBar.jsx", "src/rotas/types.ts"  ]
}
